{
  "name": "nypr-design-system-vue",
  "version": "1.0.1",
  "description": "New York Public Radio design system written in Vue",
  "repository": "nypublicradio/nypr-design-system-vue",
  "license": "MIT",
  "author": "NYPR",
  "main": "./dist/library.ssr.js",
  "module": "./dist/library.esm.js",
  "browser": "./dist/library.js",
  "unpkg": "./dist/library.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build & npm run build:rollup",
    "build:rollup": "rollup -c --environment BUILD:production",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -s ./stories -p 6006",
    "test": "jest"
  },
  "dependencies": {
    "@jest/globals": "^26.4.2",
    "@vue/composition-api": "^1.0.0-beta.14",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@jest/environment": "^26.3.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.22",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-is": "^16.13.0",
    "rollup": "^0.59.4",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "storybook-vue-router": "^1.0.7",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "gitHooks": {
    "pre-commit": "vue-cli-service lint && jest"
  },
  "homepage": "https://nypublicradio.github.io/nypr-design-system-vue"
}
